function zeroCounter(str){
    if(str.length <= 0){
        return 0;
    }
    var c=0;
    var d=0;
    for (var i=0;i<str.length;i++){
        if(str[i] == '0'){
            c++;
        }else{
            if(c>=d){
                d=c;
            }
            c=0;
        }
    }
    return d;
}

console.log(d);
